{
  "name": "localhtml-lib",
  "version": "1.5.0",
  "description": "A library for building persistent editable HTML documents.",
  "main": "docs/localhtml.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "build": "webpack --mode='production'",
    "build-debug": "webpack --mode='development' -w",
    "demo": "node demo/server.js",
    "dev": "webpack --mode=development && npm run demo",
    "nodemon": "nodemon --exec 'npm run dev' --ignore 'docs/' -e '*'",
    "stats": "webpack --mode='production' --json=docs/stats.json && webpack-bundle-analyzer docs/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidRalph/localhtml-lib.git"
  },
  "author": "David.M.L.Ralph@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DavidRalph/localhtml-lib/issues"
  },
  "homepage": "https://github.com/DavidRalph/localhtml-lib#readme",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^7.1.2",
    "css-loader": "^5.2.0",
    "file-saver": "^2.0.5",
    "html-duration-picker": "^2.2.13",
    "html-loader": "^2.1.2",
    "jquery": "^3.6.0",
    "jquery-deserialize": "^2.0.0-rc1",
    "jquery-serializejson": "^3.2.1",
    "nodemon": "^2.0.7",
    "quill": "^1.3.7",
    "rpg-dice-roller": "^5.0.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.30.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.6.0"
  }
}
